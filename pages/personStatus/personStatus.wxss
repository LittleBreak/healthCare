/* pages/personStatus.wxss */
.container{
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  display: flex;
  flex-direction: column;
  background-color: #F1F1F1;
}
.card{
  background-color: #fff;
  border-radius: 20rpx;
  width: 100%;
  padding-top: 40rpx;
}
.photo{
  width: 380rpx;
  height: 380rpx;
  border: 1px dashed red;
  border-radius: 10rpx;
}
.return-btn{
  border-radius: 10rpx;
  height: 92rpx;
  line-height: 92rpx;
  letter-spacing: 2px;
  background-color: #0C8CE5;
  font-size: 32rpx;
  color: #fff;
  font-weight: 600;
  width: 100%;
}